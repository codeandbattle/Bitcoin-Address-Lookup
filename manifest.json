{
    "name": "Bitcoin Cash Address Lookup",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Lookup Bitcoin Cash addresses from context menu.",
    "icons": {
	"16": "images/bitcoin-16.png",
	"48": "images/bitcoin-48.png",
	"128": "images/bitcoin-128.png"
    },

    "background": {
	"persistent": false,
	"scripts": [ "background.js" ]
    },
    "permissions": [
	"contextMenus"
    ]
}
